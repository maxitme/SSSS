{
  "name": "TCK",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 80",
    "lint": "yarn eslint && yarn prettier",
    "migrate": "npx prisma migrate dev",
    "undo_migrate": "npx prisma migrate reset",
    "seeder": "npx prisma db seed",
    "analyze": "ANALYZE=true next build",
    "eslint": "next lint --fix --dir src",
    "prettier": "prettier --write .",
    "format": "next lint --fix && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "postinstall": "yarn prisma generate"
  },
  "lint-staged": {
    "*.{js,jsx,md,ts,tsx,html,css,scss}": "yarn prettier",
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix --cache-location ./node_modules/.cache/.eslintcache"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.4",
    "@auth/prisma-adapter": "^1.3.2",
    "@fancyapps/ui": "^5.0.36",
    "@next/bundle-analyzer": "^14.1.0",
    "@nextui-org/react": "^2.2.10",
    "@nextui-org/table": "^2.0.28",
    "@prisma/client": "^5.12.1",
    "accept-language": "^3.0.18",
    "antd": "^5.13.2",
    "antd-style": "^3.6.1",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.3",
    "cookies": "^0.9.1",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^11.0.3",
    "i18next": "^23.8.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2",
    "i18next-resources-to-backend": "^1.2.0",
    "isomorphic-dompurify": "^2.3.0",
    "js-cookie": "^3.0.5",
    "memory-cache": "^0.2.0",
    "next": "^14.1.3",
    "next-auth": "^4.24.5",
    "next-connect": "^1.0.0",
    "next-i18next": "^15.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.2",
    "react-i18next": "^14.0.1",
    "react-icons": "^5.0.1",
    "react-intl": "^6.6.2",
    "react-select": "^5.8.0",
    "slugify": "^1.6.6",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "swiper": "^11.1.1",
    "swr": "^2.2.4",
    "use-debounce": "^10.0.0",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/i18next": "^13.0.0",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.52",
    "@types/react-dom": "^18",
    "@types/react-i18next": "^8.1.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prisma": "^5.12.1",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
